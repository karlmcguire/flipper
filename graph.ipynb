    "import png\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 179,
   "metadata": {},
   "outputs": [],
   "source": [
    "r = png.Reader(file=open(\"./data.png\", \"rb\"))\n",
    "i = r.asRGBA()\n",
    "w = i[0]\n",
    "h = i[1]\n",
    "d = i[2]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 180,
   "metadata": {},
   "outputs": [],
   "source": [
    "a = np.vstack(map(numpy.uint8, d))\n",
    "p = np.reshape(a, (h, w, 4))"
   ]
  },
