{
  "name": "flipper",
  "version": "1.0.0",
  "description": "flipper.deals frontend",
  "main": "src/index.js",
  "scripts": {
    "build": "rm -f ./docs/* && echo 'flipper.deals' > ./docs/CNAME && parcel build --no-source-maps ./src/index.html -d ./docs/",
    "serve": "rm -f ./tmp/* && parcel serve -d ./tmp/ ./src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/karlmcguire/flipper.git"
  },
  "author": "Karl McGuire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karlmcguire/flipper/issues"
  },
  "homepage": "https://github.com/karlmcguire/flipper#readme",
  "devDependencies": {
    "cssnano": "^4.1.10"
  }
}
